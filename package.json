{
	"name": "imagemin",
	"version": "8.0.1",
	"description": "Minify images seamlessly",
	"license": "MIT",
	"repository": "imagemin/imagemin",
	"type": "commonjs",
	"exports": "./index.js",
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"test": "xo && ava"
	},
	"eslintConfig": {
		"plugins": [
			"unicorn"
		]
	},
	"xo": {
		"rules": {
			"unicorn/prefer-module": 0,
			"prefer-destructuring": 0,
			"import/newline-after-import": 0,
			"unicorn/no-array-reduce": 0,
			"no-return-await": 0
		}
	},
	"files": [
		"index.js"
	],
	"keywords": [
		"minify",
		"compress",
		"image",
		"images",
		"jpeg",
		"jpg",
		"png",
		"gif",
		"svg"
	],
	"dependencies": {
		"fast-glob": "^3.2.7",
		"file-type": "^16.5.3",
		"graceful-fs": "^4.2.8",
		"junk": "^3.1.0",
		"p-pipe": "^4.0.0",
		"replace-ext": "^2.0.0",
		"slash": "^3.0.0"
	},
	"devDependencies": {
		"ava": "^3.15.0",
		"del": "^6.0.0",
		"imagemin-jpegtran": "^7.0.0",
		"imagemin-svgo": "^9.0.0",
		"imagemin-webp": "^6.0.0",
		"is-jpg": "^2.0.0",
		"tempy": "^1.0.1",
		"xo": "^0.43.0"
	}
}
